<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="navbar-inner">
    <div class="container">
      <%= link_to "Stephen Wan Accounting Corp", root_path, id: "logo" %>
      <nav>
        <ul class="nav pull-right">
          <% if cookies[:language] != nil && cookies[:language] == "CN" %>
            <%= render 'chinese/header' %>
          <% else %>
            <%= render 'english/header' %>
          <% end %>
        </ul>
      </nav>
    </div>
  </div>
</header>